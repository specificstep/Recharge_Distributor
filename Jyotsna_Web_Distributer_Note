// register receiver
        registerReceiver(smsReceiver, filter);
		
 @Override
    protected void onDestroy() {
        Log.d("Registration", "On destroy call");
        if (smsReceiver != null) {
            unregisterReceiver(smsReceiver);
            smsReceiver = null;
            Log.d("Registration", "Unregister sms receiver done");
        }
        super.onDestroy();
    }
	
